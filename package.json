{
  "name": "multrin",
  "version": "0.1.0",
  "description": "Tabbed window organizer",
  "keywords": [
    "organizer",
    "tabs",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/Sential/multrin#readme",
  "main": "main.js",
  "author": {
    "name": "Sential",
    "email": "sentialx@gmail.com"
  },
  "contributors": [
    {
      "name": "Eryk Rakowski",
      "email": "sentialx@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sential/multrin.git"
  },
  "bugs": {
    "url": "https://github.com/Sential/multrin/issues"
  },
  "scripts": {
    "start": "cross-env ENV=dev electron .",
    "watch": "concurrently \"npm run watch-renderer\" \"npm run watch-main\"",
    "watch-renderer": "webpack-dev-server --config webpack.config.renderer.dev.js",
    "watch-main": "webpack --config webpack.config.main.js --watch --mode development",
    "build-renderer": "webpack --config webpack.config.renderer.prod.js",
    "build-main": "webpack --config webpack.config.main.js --mode production",
    "build": "concurrently \"npm run build-renderer\" \"npm run build-main\"",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "devDependencies": {
    "@types/gsap": "1.19.2",
    "@types/node": "10.5.6",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "awesome-typescript-loader": "5.2.0",
    "concurrently": "3.6.1",
    "cross-env": "5.2.0",
    "electron": "4.0.0",
    "electron-builder": "20.26.1",
    "gsap": "2.0.1",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "prettier": "1.14.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "styled-components": "3.4.0",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.9.2",
    "tslint-config-prettier": "1.14.0",
    "typescript": "3.0.1",
    "url-loader": "1.0.1",
    "webpack": "4.16.4",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "4.1.4",
    "window-manager": "https://github.com/sential/node-window-manager"
  }
}
